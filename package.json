{
  "name": "pnpm-catalog-updater",
  "version": "0.1.1",
  "description": "A CLI tool to check and update pnpm workspace catalog dependencies",
  "keywords": [
    "pnpm",
    "catalog",
    "dependencies",
    "update",
    "workspace",
    "cli",
    "package-manager"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/houko/pnpm-catalog-updater.git"
  },
  "bugs": {
    "url": "https://github.com/houko/pnpm-catalog-updater/issues"
  },
  "homepage": "https://github.com/houko/pnpm-catalog-updater#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pcu": "./bin/pcu.js",
    "pnpm-catalog-updater": "./bin/pcu.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && pnpm run build:bin",
    "build:bin": "node scripts/build-bin.js",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "lint": "eslint src test --ext .ts,.js",
    "lint:fix": "eslint src test --ext .ts,.js --fix",
    "format": "prettier --write src test",
    "format:check": "prettier --check src test",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist bin/*.js",
    "prepare": "husky install",
    "prepack": "npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "libnpmconfig": "^1.2.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "npm-registry-fetch": "^16.2.1",
    "ora": "^7.0.1",
    "pacote": "^17.0.4",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.12",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "catalog:"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": ">=18.19.1",
    "pnpm": ">=8.15.0"
  }
}
